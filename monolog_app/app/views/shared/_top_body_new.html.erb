<% @user = User.new %>
  <%= form_for(@user, url: signup_path, class: 'modal-body') do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

<div class="form-group">
  <%= f.text_field :user_id, class: 'form-control',placeholder: "ユーザーID" %>
</div>
<div class="form-group">
  <%= f.email_field :email, class: 'form-control',placeholder: "メールアドレス" %>
</div>
<div class="form-group">
  <%= f.password_field :password, class: 'form-control',placeholder: "パスワード" %>
</div>
<div class="form-group">
  <%= f.password_field :password_confirmation, class: 'form-control',placeholder: "パスワード再入力" %>
</div>
  <%= recaptcha_tags %><br>
<div class="form-group">
  <%= f.submit "登録", class: "btn btn-default"%>
</div>
<% end %>

