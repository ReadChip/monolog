 <div class="container hmenu">
    <div class="row">
    
       <div class="col-lg-3">
         <%= image_tag(current_user.picture, alt: current_user.name, width: '160', height: '160') %>
       </div>

       <div class="col-lg-9">
         <div class="row">

          <div class="name">
            <div class="col-lg-1">
              <div class="glyphicon glyphicon-user"></div>
            </div>
            <div class="col-lg-11">
              <div class="husername"><%= current_user.name %></div> 
            </div>
          </div>

          <div class="comment">
            <div class="col-lg-1">
              <div class="glyphicon glyphicon-comment"></div>
            </div>
            <div class="col-lg-8">
              <div class="huserprofile"><%= current_user.profile %></div>
            </div>
          </div>

         </div>
       </div>

    </div>
</div>



<div class="container hemit">
  <div class="tab">My Emit</div>
  <div class="row">
       <%= render 'shared/feed' %>
  </div>
</div>



        